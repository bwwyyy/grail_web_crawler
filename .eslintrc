{
  "env": {
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "accessor-pairs": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "arrow-spacing": 2,
    "babel/arrow-parens": 0,
    "block-scoped-var": 0,
    "constructor-super": 2,
    "consistent-return": 2,
    "curly": [2, "all"],
    "default-case": 0,
    "dot-location": 0,
    "dot-notation": [2, { "allowKeywords": true }],
    "eqeqeq": 2,
    "generator-star-spacing": 0,
    "guard-for-in": 0,
    "no-alert": 2,
    "no-confusing-arrow": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-div-regex": 0,
    "no-dupe-class-members": 2,
    "no-else-return": 0,
    "no-empty-pattern": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 0,
    "no-implicit-coercion": 0,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-labels": [2, {"allowLoop": true, "allowSwitch": true}],
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-magic-numbers": 0,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-process-env": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-var": 2,
    "no-void": 0,
    "no-warning-comments": 0,
    "no-with": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 1,
    "prefer-const": 2,
    "prefer-reflect": 0,
    "prefer-spread": 2,
    "prefer-template": 1,
    "radix": 0,
    "require-yield": 2,
    "semi": 2,
    "vars-on-top": 0,
    "wrap-iife": 0,
    "yoda": [2, "never"]
  }
}